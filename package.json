{
  "name": "p2pu-react-components",
  "version": "0.0.0",
  "description": "React components used by P2PU apps",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run build:style && npm run build:js",
